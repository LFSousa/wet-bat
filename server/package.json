{
  "name": "server",
  "version": "1.0.0",
  "author": {
    "name": "Luis Filipe Sousa",
    "email": "luis@filipe.xyz"
  },
  "scripts": {
    "build": "tsc --project tsconfig.compile.json",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "yarn prisma:generate && cross-env NODE_ENV=production node dist/src/index.js",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@tsed/ajv": "^6.114.14",
    "@tsed/common": "^6.114.14",
    "@tsed/core": "^6.114.14",
    "@tsed/di": "^6.114.14",
    "@tsed/engines": "^6.114.14",
    "@tsed/exceptions": "^6.114.14",
    "@tsed/json-mapper": "^6.114.14",
    "@tsed/logger": "^6.1.3",
    "@tsed/logger-file": "^6.1.3",
    "@tsed/platform-cache": "^6.114.14",
    "@tsed/platform-exceptions": "^6.114.14",
    "@tsed/platform-express": "^6.114.14",
    "@tsed/platform-log-middleware": "^6.114.14",
    "@tsed/platform-middlewares": "^6.114.14",
    "@tsed/platform-params": "^6.114.14",
    "@tsed/platform-response-filter": "^6.114.14",
    "@tsed/platform-views": "^6.114.14",
    "@tsed/prisma": "^6.114.14",
    "@tsed/schema": "^6.114.14",
    "@tsed/swagger": "^6.114.14",
    "ajv": "^8.11.0",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.1",
    "method-override": "^3.0.0",
    "ts-node": "^10.8.0"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.20.17",
    "@tsed/cli-plugin-prisma": "3.20.17",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default",
    "architecture": "default"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}